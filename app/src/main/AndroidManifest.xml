<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ai_bot.layouts">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".memberListActivity"
            android:label="@string/title_member_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MemberListActivity"
            android:label="@string/title_activity_member_list"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.ai_bot.layouts.MainActivity" />
        </activity>
        <activity
            android:name=".memberDetailActivity"
            android:label="@string/title_member_detail"
            android:parentActivityName=".memberListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.ai_bot.layouts.memberListActivity" />
        </activity>
        <activity
            android:name=".AttendanceReport"
            android:label="@string/title_activity_attendance_report"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.ai_bot.layouts.MainActivity" />
        </activity>
        <activity
            android:name=".fees"
            android:label="@string/title_activity_fees"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.ai_bot.layouts.MainActivity" />
        </activity>
        <activity
            android:name=".attendance_mark"
            android:label="@string/title_activity_attendance_mark"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.ai_bot.layouts.MainActivity" />
        </activity>
    </application>

</manifest>